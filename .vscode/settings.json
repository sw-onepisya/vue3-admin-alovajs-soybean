{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.formatOnSave": false,
  "eslint.validate": ["html", "css", "scss", "json", "jsonc"],
  "i18n-ally.enabledParsers": ["ts"],
  "i18n-ally.keystyle": "flat",
  "i18n-ally.localesPaths": ["src/locales/langs"],
  "i18n-ally.disabled": false,
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.pathMatcher": "{locale}.ts",
  "i18n-ally.displayLanguage": "zh-cn",
  "i18n-ally.sourceLanguage": "zh-cn",
  "i18n-ally.translate.engines": ["google-cn", "google", "deepl"],
  "i18n-ally.sortKeys": true, // 字母顺序排序
  "i18n-ally.keepFulfilled": true, // 始终用空字符串填充所有的键
  "i18n-ally.enabledFrameworks": [
    // 指定框架
    "vue"
  ],
  "i18n-ally.extract.targetPickingStrategy": "file-previous",
  "i18n-ally.editor.preferEditor": false, // 如果是 js 或者 ts 格式、会自动 readoonly 不允许修改、只能自己去修改源文件
  "prettier.enable": false,
  "unocss.root": ["./"],
  "vue.server.hybridMode": true,
  "i18n-ally.extract.keyMaxLength": 80,
  "i18n-ally.fullReloadOnChanged": true,
}
